{
    "name": "loveduckie/silverstripe-webp-image",
    "description": "Automatically generate and serve .webp images from existing .jpeg, .png, and .gif files.",
    "keywords": [
        "silverstripe",
        "webp",
        "image"
    ],
    "version": "1.0.1",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Luc Shelton",
            "email": "lucshelton@gmail.com",
            "homepage": "https://www.lucshelton.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "lucshelton@gmail.com",
        "issues": "https://github.com/loveduckie/silverstripe-webp-image/issues"
    },
    "extra": {
        "installer-name": "webp-image"
    },
    "autoload": {
        "psr-4": {
            "LoveDuckie\\Silverstripe\\WebPImage\\": "src/",
            "LoveDuckie\\Silverstripe\\WebPImage\\Tests\\": "tests/"
        }
    },
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.10 | ^5.0"
    },
    "extra": {
        "installer-name": "silverstripe-webp-image",
        "screenshots": [],
        "expose": [
            "client/dist"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
